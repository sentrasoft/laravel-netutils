{
    "name": "sentrasoft/laravel-netutils",
    "description": "Network Utilities for Laravel",
    "keywords": [
        "ip",
        "ping",
        "network",
        "laravel",
        "lan",
        "wake-on-lan"
    ],
    "homepage": "https://github.com/sentrasoft/laravel-netutils",
    "support": {
        "issues": "https://github.com/sentrasoft/laravel-netutils/issues",
        "source": "https://github.com/sentrasoft/laravel-netutils"
    },
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Fahmi L. Ramdhani",
            "email": "fahmilestianramdhani@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "~5.4"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Sentrasoft\\Netutils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sentrasoft\\Netutils\\NetutilsServiceProvider"
            ],
            "aliases": {
                "Netutils": "Sentrasoft\\Netutils\\Facades\\Netutils"
            }
        }
    }
}
